{
  "name": "room",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "copy-repos": "node ./node-scripts/copy-repos",
    "ng": "ng",
    "env-dev": "node ./node-scripts/set-env --envkey=dev",
    "env-local": "node ./node-scripts/set-env --envkey=local && npm run config-ng1",
    "env-localdev": "node ./node-scripts/set-env --envkey=localdev && npm run config-ng1-localdev",
    "env-test": "node ./node-scripts/set-env --envkey=test && npm run config-ng1-test",
    "env-k8s": "node ./node-scripts/set-env --envkey=k8s && npm run config-ng1-k8s",
    "env-k8sdev": "node ./node-scripts/set-env --envkey=k8sdev && npm run config-ng1-k8sdev",
    "env-env17": "node ./node-scripts/set-env --envkey=env17 && npm run config-ng1-env17",
    "env-stag": "node ./node-scripts/set-env --envkey=stag && npm run config-ng1-stag",
    "env-stagdev": "node ./node-scripts/set-env --envkey=stagdev && npm run config-ng1-stagdev",
    "env-live": "node ./node-scripts/set-env --envkey=live && npm run config-ng1-live",
    "setup": "npm run env-dev && npm run env-local && rm -rf src/lib-components && npm run copy-repos",
    "setup-stag": "npm run env-dev && npm run env-stagdev && rm -rf src/lib-components && npm run copy-repos",
    "setup-k8sdev": "npm run env-dev && npm run env-k8sdev && rm -rf src/lib-components && npm run copy-repos",
    "setup-env17": "npm run env-dev && npm run env-env17 && rm -rf src/lib-components && npm run copy-repos",
    "setup-localdev": "npm run env-localdev && rm -rf src/lib-components && npm run copy-repos",
    "start-local": "npm run setup && ng serve --configuration=local --host 0.0.0.0 --disable-host-check",
    "start-http": "npm run start-stag",
    "start": "npm run setup-stag && ng serve --configuration=stagdev --host 0.0.0.0 --disable-host-check --ssl=true --ssl-key=dev-ssl/pl-dev-ssl.key --ssl-cert=dev-ssl/pl-dev-ssl.crt",
    "start-localdev": "npm run setup-localdev && ng serve --configuration=localdev",
    "start-stag": "npm run setup-stag && ng serve --configuration=stagdev",
    "start-k8sdev": "npm run setup-k8sdev && ng serve --configuration=k8sdev",
    "start-env17": "npm run setup-env17 && ng serve --configuration=env17 --host 0.0.0.0 --disable-host-check --ssl=true",
    "ipad-dev": "npm start",
    "ng-build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod --configuration=$NG_ENV --statsJson",
    "ng-build-BAD2": "ng build --configuration=$NG_ENV --statsJson",
    "ng-build-BAD": "ng build --configuration=$NG_ENV --aot --statsJson",
    "build": "ng build",
    "build-local": "npm run setup && npm run env-local && NG_ENV=local npm run ng-build",
    "build-localdev": "npm run setup-localdev && npm run env-localdev && NG_ENV=localdev npm run ng-build",
    "build-test": "npm run setup && npm run env-test && NG_ENV=test npm run ng-build",
    "build-k8s": "npm run setup && npm run env-k8s && NG_ENV=k8s npm run ng-build",
    "build-env17": "npm run setup && npm run env-env17 && NG_ENV=env17 npm run ng-build",
    "build-stag": "npm run setup && npm run env-stag && NG_ENV=stag npm run ng-build",
    "build-live": "npm run setup && npm run env-live && NG_ENV=live npm run ng-build",
    "start-build": "node node-server-webpack.js",
    "config-ng1": "node ./node-scripts/config-ng1",
    "config-ng1-test": "ENVKEY=test npm run config-ng1",
    "config-ng1-k8s": "ENVKEY=k8s npm run config-ng1",
    "config-ng1-k8sdev": "ENVKEY=k8sdev npm run config-ng1",
    "config-ng1-env17": "ENVKEY=env17 npm run config-ng1",
    "config-ng1-stag": "ENVKEY=stag npm run config-ng1",
    "config-ng1-stagdev": "ENVKEY=stagdev npm run config-ng1",
    "config-ng1-localdev": "ENVKEY=localdev npm run config-ng1",
    "config-ng1-live": "ENVKEY=live npm run config-ng1",
    "svg": "node ./node-scripts/svg",
    "test-data-setup": "node ./node-scripts/node-data-setup.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
